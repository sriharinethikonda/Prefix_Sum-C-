#include<iostream>
#include<bits/stdc++.h>
using namespace std;
long long int N=1000001;
long long int seive[1000001];
void gen_seive(){
    seive[0]=seive[1]=1;
	for (int i=2;i<N;i++)
		seive[i]=i;
	for(int i=2;i*i<N;i++){
	   			if(seive[i]==i){
				    for(int j=i*i;j<=N;j+=i){
				    if(seive[j]%j==0)
					seive[j]=i;
			        }
	}
}
}

int main(){
gen_seive();
int q;
cin>>q;
while(q--){
    int n;
    cin>>n;
    int count=0;
    for(int i=2;i<=N;i++){
      if(seive[i]==n)
         count++;
    }
    cout<<count<<"\n";
}
   

return 0;
}
