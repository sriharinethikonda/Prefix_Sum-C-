#include<iostream>
using namespace std;
long long int N=1000001;
long long int seive[1000001];
void gen_seive(){
	for (int i=0;i<N;i++)
		seive[i]=1;
		seive[0]=seive[1]=0;
		for(int i=2;i*i<N;i++){
			if(seive[i]==1){
				for(int j=i*i;j<=N;j+=i)
					seive[j]=0;
			}
		}
	}
int main(){
gen_seive();
int n;
cin>>n;
int count=0;
if(n==0||n==1)
    count=0;
else{
for(int i=2;i<=n;i++){
	if(seive[i]==1)	
		count++;
}
}
cout<<count<<" ";
return 0;
}
